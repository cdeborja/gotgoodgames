<h2>Sign In</h2>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>
  <div class="sign-in-box group">
  <label class="input">
    Username
    <input type="text" name="user[username]" value="">
  </label>

  <label class="input">
    Password
    <input type="password" name="user[password]" value="">
  </label>

  <input class="submit-button" type="submit" value="Sign In">
</div>
</form>
